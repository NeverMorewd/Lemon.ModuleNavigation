<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="Lemon.ModuleNavigation.Sample.Views.ViewAlpha"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:lm="https://github.com/NeverMorewd/Lemon.ModuleNavigation"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border BorderBrush="LemonChiffon" BorderThickness="2">
        <StackPanel>
            <TextBlock Text="{Binding Greeting}" />
            <Button
                Command="{Binding CloseCommand}"
                Content="CloseDialog"
                Height="50"
                IsVisible="{Binding IsDialog}"
                Width="200" />
            <!--  <Button  -->
            <!--  Width="200"  -->
            <!--  Height="50"  -->
            <!--  lm:NavigationExtension.CanUnload="True"  -->
            <!--  Content="UnloadView"  -->
            <!--  IsVisible="{Binding !IsDialog}" />  -->
        </StackPanel>
    </Border>
</UserControl>
